beta_post[j] <-  rnorm(1,mean = mu_beta[j], sd = tau_beta[j]^-0.5)
alpha_pre[j] <-  rnorm(1, mean = rnorm(1, mean = 0, sd = sqrt(1000)),
sd = rgamma(1,shape = 1, rate = 1)^-0.5)
beta_pre[j] <-   rnorm(1, mean = rnorm(1,mean = 0, sd = sqrt(1000)),
sd = rgamma(1,shape = 1,rate = 1)^-0.5)
}
